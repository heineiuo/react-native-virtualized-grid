{
  "name": "react-native-virtualized-grid",
  "version": "0.7.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build:commonjs": "babel src --out-dir lib/commonjs --extensions '.ts,.tsx' --source-maps",
    "build:module": "tsc src/*.tsx --module es2015 --target es6 --jsx 'react-native' --sourceMap --skipLibCheck --allowSyntheticDefaultImports --outDir lib/module",
    "build:typescript": "tsc src/*.tsx --declaration --target es6 --emitDeclarationOnly --jsx 'react-native'  --skipLibCheck --allowSyntheticDefaultImports  --outDir lib/typescript",
    "prepublishOnly": "node scripts/prepublishOnly.js",
    "postpublish": "node scripts/postpublish.js",
    "build:web": "expo build:web",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "types": "lib/typescript/index.d.ts",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib"
  ],
  "description": "A Table component for both react-native and react-native-web.",
  "keywords": [
    "react-native",
    "react-native-virtualized-grid"
  ],
  "author": "heineiuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heineiuo/react-native-virtualized-grid.git"
  },
  "bugs": {
    "url": "https://github.com/heineiuo/react-native-virtualized-grid/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "eslint-config-react-app": "^7.0.0",
    "eslint-config-universe": "^10.0.0",
    "expo": "^44.0.0",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1",
    "typescript": "~4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "universe/native",
      "universe/web",
      "react-app"
    ]
  }
}
