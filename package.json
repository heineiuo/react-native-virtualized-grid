{
  "name": "react-native-virtualized-grid",
  "version": "0.2.2",
  "type": "module",
  "scripts": {
    "build": "tsc src/*.tsx  --declaration --module es2015 --target es6 --jsx 'react-native' --sourceMap --skipLibCheck --allowSyntheticDefaultImports --outDir lib",
    "prepublishOnly": "node scripts/prepublishOnly.js",
    "postpublish": "node scripts/postpublish.js",
    "build:web": "expo export:web",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "description": "A Table component for both react-native and react-native-web.",
  "keywords": [
    "react-native",
    "react-native-virtualized-grid"
  ],
  "author": "heineiuo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heineiuo/react-native-virtualized-grid.git"
  },
  "bugs": {
    "url": "https://github.com/heineiuo/react-native-virtualized-grid/issues"
  },
  "homepage": "https://heineiuo.github.io/react-native-virtualized-grid",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@expo/metro-runtime": "~3.1.3",
    "@expo/webpack-config": "^19.0.1",
    "@types/react": "~18.2.45",
    "@types/react-native": "0.70.8",
    "expo": "^50.0.11",
    "expo-status-bar": "~1.11.1",
    "react-native": "0.73.4",
    "react-native-web": "~0.19.6",
    "typescript": "^5.3.0"
  }
}
